<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(NetFrameworkVersion);$(NetStandardVersion)</TargetFrameworks>
    <PackageTags>HealthCheck;IbmMQ</PackageTags>
    <Description>HealthChecks.IbmMQ is the health check package for IbmMQ.</Description>
    <Version>$(HealthCheckIbmMQ)</Version>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="IBMMQDotnetClient" Version="9.2.3" />
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="6.0.0" />
    <Compile Include="../HealthCheckResultTask.cs" />
  </ItemGroup>

</Project>
