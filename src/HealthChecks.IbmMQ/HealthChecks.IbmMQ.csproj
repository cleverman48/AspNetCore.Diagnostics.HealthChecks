<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(NetFrameworkVersion);$(NetStandardVersion)</TargetFrameworks>
    <PackageTags>$(PackageTags);IbmMQ</PackageTags>
    <Description>HealthChecks.IbmMQ is the health check package for IbmMQ.</Description>
    <Version>$(HealthCheckIbmMQ)</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="IBMMQDotnetClient" Version="9.3.0" />
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="6.0.6" />
    <Compile Include="../HealthCheckResultTask.cs" />
  </ItemGroup>

</Project>
