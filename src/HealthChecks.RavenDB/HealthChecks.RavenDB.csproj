<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageTags>$(PackageTags);RavenDB</PackageTags>
    <Description>HealthChecks.RavenDB is the health check package for RavenDB.</Description>
    <VersionPrefix>$(HealthCheckRavenDB)</VersionPrefix>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="7.0.1" />
    <PackageReference Include="RavenDB.Client" Version="5.4.100" />
  </ItemGroup>

</Project>
