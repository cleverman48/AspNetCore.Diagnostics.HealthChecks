<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>$(NetFrameworkVersion);$(NetStandardVersion)</TargetFrameworks>
    <PackageTags>HealthCheck;Health;RavenDB</PackageTags>
    <Description>HealthChecks.RavenDB is the health check package for RavenDB.</Description>
    <Version>$(HealthCheckRavenDB)</Version>
    <PackageId>AspNetCore.HealthChecks.RavenDB</PackageId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" />
    <PackageReference Include="RavenDB.Client"/>
  </ItemGroup>
</Project>
