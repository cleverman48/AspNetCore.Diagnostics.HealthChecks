<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(NetFrameworkVersion);$(NetStandardVersion)</TargetFrameworks>
    <PackageTags>HealthCheck;Health;RavenDB</PackageTags>
    <Description>HealthChecks.RavenDB is the health check package for RavenDB.</Description>
    <Version>$(HealthCheckRavenDB)</Version>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="6.0.0" />
    <PackageReference Include="RavenDB.Client" Version="5.2.3" />
  </ItemGroup>

</Project>
