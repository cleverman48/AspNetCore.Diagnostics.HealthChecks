<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(NetFrameworkVersion);$(NetStandardVersion)</TargetFrameworks>
    <PackageTags>HealthCheck;Health;Oracle</PackageTags>
    <Description>HealthChecks.Oracle is the health check package for Oracle Database.</Description>
    <Version>$(HealthCheckOracle)</Version>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Oracle.ManagedDataAccess.Core" Version="2.19.130" />
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="6.0.3" />
  </ItemGroup>

</Project>
