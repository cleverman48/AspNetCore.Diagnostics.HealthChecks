<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(NetFrameworkVersion);$(NetStandardVersion)</TargetFrameworks>
    <PackageTags>HealthCheck;Azure;CosmosDb</PackageTags>
    <Description>HealthChecks.CosmosDb is the health check package for Azure CosmosDb.</Description>
    <Version>$(HealthCheckCosmosDb)</Version>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.Cosmos" Version="3.25.0" />
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="6.0.2" />
    <PackageReference Include="Azure.Data.Tables" Version="12.4.0" />
  </ItemGroup>

</Project>
