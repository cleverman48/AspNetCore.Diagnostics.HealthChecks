<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageTags>$(PackageTags);DynamoDb;AWS</PackageTags>
    <Description>HealthChecks.DynamoDb is the health check package for AWS DynamoDb.</Description>
    <VersionPrefix>$(HealthCheckDynamoDb)</VersionPrefix>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.DynamoDBv2" Version="3.7.101.9" />
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="7.0.3" />
  </ItemGroup>

</Project>
