<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageTags>$(PackageTags);MySQL</PackageTags>
    <Description>HealthChecks.MySql is the health check package for MySQL.</Description>
    <VersionPrefix>$(HealthCheckMySql)</VersionPrefix>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MySqlConnector" Version="2.2.4" />
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="7.0.1" />
  </ItemGroup>

</Project>
