<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>$(NetStandardTargetVersion)</TargetFramework>
    <Description>HealthChecks.Network is the health check package for network services.</Description>
    <PackageTags>HealthCheck;Health;Network;Sftp;Ftp;Tcp;DNS</PackageTags>
    <Version>$(HealthCheckNetwork)</Version>
    <LangVersion>latest</LangVersion>
    <PackageId>AspNetCore.HealthChecks.Network</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SSH.NET" Version="$(SSHNet)" />
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="$(MicrosoftExtensionsDiagnosticsHealthChecks)" />
    <PackageReference Include="System.Buffers" Version="$(SystemBuffers)" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="$(MicrosoftSourceLinkGithub)">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>


</Project>
